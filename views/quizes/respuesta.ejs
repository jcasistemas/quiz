<p id="<%= respuesta %>"><%= respuesta %> !!!</p>
</br>
<%
switch( respuesta ) {
	case "Incorrecto": %>
		<a href="/quizes/<%= quiz.id %>"><button class="inv">Intentelo de nuevo</button></a>
<%
		break;
	case "Correcto": %>
		<a href="/quizes"><button class="inv">Responder mas preguntas</button></a>
<%
		break;
	case "Error": %>
	    <p id="Error1">No existe la pregunta <%= quizId %><p></br>
		<a href="/quizes"><button class="inv">Ver Preguntas</button></a>
<%
		break;
	default:
}
%>
